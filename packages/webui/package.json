{
    "private": true,
    "name": "id.gojabako.zone",
    "siteName": "Your ID on gojabako.zone",
    "scripts": {
        "test": "run-s test:*",
        "test:type": "tsc --noEmit",
        "dev": "run-s dev:*",
        "dev:watch": "run-p dev:watch:*",
        "dev:watch:css": "esifycss --css=src/app.css --ext=.ts --watch src",
        "dev:watch:next": "node server.mjs",
        "build": "run-s build:*",
        "build:aws": "run-s build:aws:*",
        "build:aws:buildLambda": "ts-node aws/scripts/buildLambda.ts",
        "build:aws:cdk": "cdk --app \"npx ts-node aws/cdk.ts\" deploy --all --require-approval never --outputs-file cdk.out/output.json",
        "build:aws:createEnv": "ts-node aws/scripts/createEnv.ts",
        "build:css": "esifycss --css=src/app.css --ext=.ts src",
        "build:next": "next build",
        "destroy": "cdk --app \"npx ts-node aws/cdk.ts\" destroy --all",
        "start": "NODE_ENV=production node server.mjs",
        "lint": "next lint",
        "generatePolicyDocument": "ts-node aws/scripts/generatePolicyDocument.ts"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "3.37.0",
        "@aws-sdk/client-iam": "3.37.0",
        "@aws-sdk/client-sts": "3.37.0",
        "@aws-sdk/util-dynamodb": "3.37.0",
        "@id.gojabako.zone/util-es": "file:../util-es"
    },
    "devDependencies": {
        "esifycss": "1.4.35",
        "esifycss-webpack-plugin": "0.1.3",
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "browserslist": [
        ">5%",
        "not ie 11",
        "not dead"
    ],
    "jest": {
        "preset": "ts-jest/presets/default-esm",
        "collectCoverage": true
    }
}
