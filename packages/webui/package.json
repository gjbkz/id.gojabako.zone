{
    "private": true,
    "name": "@id.gojabako.zone/webui",
    "scripts": {
        "test": "run-s test:*",
        "test:type": "tsc --noEmit",
        "dev": "run-p dev:*",
        "dev:css": "esifycss --css=src/app.css --ext=.ts --watch src",
        "dev:next": "node server.mjs",
        "build": "run-s build:*",
        "build:css": "esifycss --css=src/app.css --ext=.ts src",
        "build:next": "next build",
        "start": "NODE_ENV=production node server.mjs"
    },
    "devDependencies": {
        "esifycss": "1.4.35",
        "esifycss-webpack-plugin": "0.1.3",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "@id.gojabako.zone/util-es": "file:../util-es"
    },
    "browserslist": [
        ">5%",
        "not ie 11",
        "not dead"
    ],
    "jest": {
        "preset": "ts-jest/presets/default-esm",
        "collectCoverage": true
    }
}
